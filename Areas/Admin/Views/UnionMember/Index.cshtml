@model IEnumerable<UnionMember>
@{
	ViewData["Title"] = "Anv√§ndare";
}
<div class="col-md-9">
	<div class="alert alert-success alert-dismissable">
		<a href="#" class="close" data-dismiss="alert" aria-label="close">&times;</a>
		<strong>Success!</strong> @ViewData["Message"]
	</div>
	<a asp-action="Create" id="addMember" class="btn btn-default">Add New User</a>
	<table class="table table-striped table-hover table-row-click">
		<thead>
			<tr>
				<th>
					<!--@Html.DisplayNameFor(model => model.Name)-->
				</th>
				<th>
					<!--@Html.DisplayNameFor(model => model.Title)-->
				</th>
				<th>
					<!--@Html.DisplayNameFor(model => model.Phone)-->
				</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			@foreach (var user in Model) {
				<tr>
					<td>
						@Html.DisplayFor(modelItem => user.Name)
					</td>
					<td>
						@Html.DisplayFor(modelItem => user.Title)
					</td>
					<td>
						@Html.DisplayFor(modelItem => user.Phone)
					</td>
					<td>
						<a asp-action="Edit" asp-route-id="@user.Id">Edit</a> |
						<a asp-action="Delete" asp-route-id="@user.Id">Delete</a>
					</td>
				</tr>
			}
		</tbody>
	</table>
</div>