@model IEnumerable<ApplicationUser>
@{
	ViewData["Title"] = "Användare";
}
	<h2>@ViewData["Title"]</h2>
	<h3>@ViewData["Message"]</h3>
	
	<!--<form asp-action="Index" method="get" class="form-inline">
